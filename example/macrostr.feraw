cc {
include("../lib/std.c");
};

Vector2 = macro{
    list(0);
    dup();
    set(remove(stack, -3), "x", $0);
    dup();
    set(remove(stack, -3), "y", $1);
};

start:
a = Vector2(1, 2);
println("Vector2 Example");
println(a.x);
println(a.y);
a.x = 3;
println(a.x);
a.y = 4;
println(a.y);
println(a);