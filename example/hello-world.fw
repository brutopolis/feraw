// ifgo is the only control structure that exists in feraw
// we can use it as if:
?(condition, conditiontrue);
// as else:
?(1, conditionfalse);

conditiontrue:
    print("true");
    // as a goto:
    ?(1, aftercondition);

conditionfalse:
    print("false");

aftercondition:

// as done/break:
?(1, end);

// goto start;
?(1, start);

end: