macro_print = macro(
    print($0);
);

// macro creating + assign it to macro_example name
macro_example = macro(
    macro_print($0);
    macro_print($1);
);

// call the macro_example macro with 9 and 10 as arguments
// is expected to print 9 then 10
macro_example(9, 10);

